<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>soss-ros2-test</name>
  <version>2.0.0</version>
  <description>The SOSS package to test ros2</description>

  <maintainer email="grey@openrobotics.org">Grey</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>yaml-cpp</build_depend>

  <depend>soss-core</depend>
  <depend>soss-mock</depend>
  <depend>soss-rosidl</depend>
  <depend>soss-ros2</depend>
  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>test_msgs</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
